 from 'uuid'\nimport Container from 'react-bootstrap/Container'\nimport { addToFavorites } from '../actions/games'\nimport { connect } from 'react-redux'\nimport ReviewsContainer from '../containers/ReviewsContainer';\nimport Button from 'react-bootstrap/button'\nimport { toast } from 'react-toastify'\nimport crt2 from '../images/crt2.png'\n\n\ntoast.configure()\n\nconst GameShow = ({game, screenshots, addToFavorites, user, games}) => {\n\n  const handleFavorite = () => {\n    \n    if (!user) {\n      return  toast.error(\"You must be logged in to add favorites\", {position: \"top-center\", autoClose: 3000})\n    } \n\n    const alreadyFavorite = games.find(g => g.game_api_id === game.id)\n\n    if (alreadyFavorite) {\n        return toast.info(`This game is already in ${user.username}'s collection!`, {position: \"top-center\", autoClose: 3000})\n      } else {\n      addToFavorites(game.id, game.name, game.background_image, user)\n    }\n  }\n\n\n    return (\n      <Container className=\"mt-5 \">\n        {/* needs styling */}\n        \n        <div style={{textAlign: \"center\"}}>\n          \n          <img style={{height: \"50%\", width: \"50%\"}} src={game.background_image} alt=\"game\" />\n\n          <h1>{game.name}</h1>\n          <p>Released: {game.released}</p>\n          <p>Platform(s):</p>\n            {\n            game.platforms.map(p => <span key={uuidv4()}>{`${p.platform.name} | ` }</span>)\n            }\n            <br>\n            </br>\n            <Button onClick={handleFavorite}>Add to Favorites</Button>\n        </div>\n       <div style={{textAlign: \"center\"}}>\n    \n        { \n        game.clip === null ? \n          null \n        :\n          <video src={game.clip.clip} type=\"video/mp4\" controls/>\n        }\n    \n        <p style={{textAlign: \"left\"}} >{game.description_raw}</p>\n  \n        {/* <img src={game.background_image} alt='game'/>\n  \n        <img src= {game.background_image_additional} alt='game'/> */}\n\n        { \n          screenshots ?\n          screenshots.map(ss => <img src={ss.image} style={{height: \"16em\", width: \"20em\", margin: \"1%\"}} alt='screenshot' key={uuidv4()}></img>)\n          :\n          null\n        }\n        </div>\n        <ReviewsContainer gameApiId={game.id} gameName={game.name} gameImage={game.background_image} />\n  \n      </Container >\n    ); \n}\n\nconst mapStateToProps = (state) => {\n  return {\n    user: state.user,\n    games: state.games\n  }\n}\n\nexport default connect (mapStateToProps, { addToFavorites })(GameShow)\n\n\n\n","/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/App.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/components/NavConsoleList.js",[],["169","170"],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/components/Login.js",[],["171","172"],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/components/Signup.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/index.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/containers/GameContainer.js",[],["173","174"],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/reducers/gamesReducer.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/components/FavoriteGame.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/components/Review.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/containers/ReviewsContainer.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/reducers/index.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/reducers/reviewsReducer.js",[],"/Users/kylefarmer/Flatiron/mod5/gamepad mod 5 project/gamepad-react-frontend/src/actions/re